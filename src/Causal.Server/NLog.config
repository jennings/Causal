<?xml version="1.0"?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <targets>
    <target name="main" xsi:type="File" archiveNumbering="Sequence" fileName="${basedir}\App_Data\Causal.Server.log"/>
  </targets>
  <rules>
    <logger name="*" minlevel="Debug" maxlevel="Fatal" writeTo="main"/>
  </rules>
</nlog>